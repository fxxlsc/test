.nav{
  width: 100%;
  height: 50px;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
}
.nav navigator{
  width: 25%;
  height: 100%;
}
.navbox{
  height: 140rpx;
  text-align: center;
  position: relative;
}
.navtext{
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  margin-left:-28rpx;
  font-size: 28rpx; 
}
.navicon{
  border-radius:50%; 
  border: 1px solid #eee;
  width: 81rpx;
  height: 81rpx;
}